{
  "name": "dev-blog",
  "version": "0.0.0",
  "scripts": {
    "app:serve": "npm run scully:scan && ng serve",
    "app:build": "npm run app:clean && npm run angular:build && npm run scully:build",
    "app:clean": "npm run angular:clean && npm run scully:clean",
    "ng": "ng",
    "angular:start": "ng serve",
    "angular:build": "ng build",
    "angular:watch": "ng build --watch --configuration development",
    "angular:test": "ng test",
    "angular:lint": "ng lint",
    "angular:clean": "rm -rf dist/*",
    "scully": "npx scully --",
    "scully:start": "npx scully serve",
    "scully:build": "npm run scully:clean && npx scully",
    "scully:scan": "npx scully --scanRoutes",
    "scully:clean": "rm -rf docs/*",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "^12.2.12",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/material": "^12.2.12",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@scullyio/init": "^2.1.3",
    "@scullyio/ng-lib": "^2.1.3",
    "@scullyio/platform-server": "^2.1.3",
    "@scullyio/scully": "^2.1.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@scullyio/scully-plugin-base-href-rewrite": "^2.1.3",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.4"
  }
}